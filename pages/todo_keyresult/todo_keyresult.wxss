/* pages/todo_keyresult/todo_keyresult.wxss */
.page-container {
  height: 100%;
}
.todo_keyresult-title {
	font-size: 13px;
  color: #999;
  background: #fff;
  padding-top: 20rpx;
  padding-bottom: 30rpx;
  position: sticky;
  top: 0;
  z-index: 10;
}
.todo_keyresult-bg {
  width: 100%;
	font-size: 14px;
	color: #ff5145;
	text-align: center;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
}
.todo_keyresult-image {
	width: 200rpx;
	height: 200rpx;
	margin-bottom: 30rpx;
}
.todo_keyresult-list {
  padding-bottom: 10rpx;
}
.todo_keyresult-item-title {
  font-size: 15px;
  color: #ff5145;
	margin-bottom: 10rpx;
}
.todo_keyresult-btn {
  font-size: 14px;
	text-align: left;
	border: 1px solid #ff5145;
	padding: 10rpx 10rpx;
	line-height: 50rpx;
  margin-bottom: 30rpx;
}
.todo_keyresult-btn.active {
  color: #fff;
  background: #ff5145;
}